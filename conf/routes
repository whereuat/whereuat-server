# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.ViewController.index

# Casbah reference route
GET     /read                       controllers.Application.read

# Dummy routes
GET     /account/new                controllers.ViewController.newAccount
GET     /at                         controllers.ViewController.at
GET     /where                      controllers.ViewController.where

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
